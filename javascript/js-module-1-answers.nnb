            "source": [
                "const nFactorial = (num) => {\r\n   if (typeof num !== \"number\") return null;\r\n   let result = 1;\r\n   for (let index = 1; index <= num; index++) {\r\n      result *= index;\r\n   }\r\n\r\n   return result;\r\n};"
